{
  "name": "isaac-react-app",
  "version": "1.6.22-SNAPSHOT",
  "private": true,
  "engines": {
    "node": ">=17",
    "npm": ">=8"
  },
  "engineStrict": true,
  "dependencies": {
    "@react-google-maps/api": "^2.7.0",
    "axios-mock-adapter": "^1.20.0",
    "billboard.js": "^1.12.11",
    "core-js": "^3.19.3",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "file-loader": "^6.2.0",
    "he": "^1.2.0",
    "highlight.js": "^11.3.1",
    "history": "^4.10.1",
    "html-to-text": "^8.1.0",
    "inequality": "^0.9.33",
    "inequality-grammar": "^0.10.3",
    "isaac-graph-sketcher": "^0.9.4",
    "js-cookie": "^3.0.1",
    "katex": "^0.15.1",
    "lodash": "^4.17.21",
    "qrcode": "^1.5.0",
    "query-string": "^7.0.1",
    "rand-seed": "^1.0.1",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-circular-progressbar": "^2.0.4",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-ga": "^3.3.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-select": "^5.2.1",
    "reactstrap": "^8.10.1",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.4.1",
    "remarkable": "^2.0.1",
    "string.prototype.matchall": "^4.0.6",
    "url-loader": "^4.1.1"
  },
  "scripts": {
    "build-phy": "webpack --env prod --config config/webpack.config.physics.js",
    "build-cs": "webpack --env prod --config config/webpack.config.cs.js",
    "build-phy-dev": "webpack --config config/webpack.config.physics.js",
    "build-cs-dev": "webpack --config config/webpack.config.cs.js",
    "start-phy": "webpack-dev-server --hot --port 8004 --history-api-fallback --config config/webpack.config.physics.js --allowed-hosts=true",
    "start-cs": "webpack-dev-server --hot --port 8003 --history-api-fallback --config config/webpack.config.cs.js --allowed-hosts=true",
    "test": "npm run test-phy && npm run test-cs",
    "test-phy": "jest --config config/jest/jest.config.physics.js",
    "test-cs": "jest --config config/jest/jest.config.cs.js",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "build-stats-phy": "webpack --env prod --config config/webpack.config.physics.js --profile --json > bundle-stats-phy.json",
    "analyse-bundle-phy": "webpack-bundle-analyzer bundle-stats-phy.json build-physics",
    "build-stats-cs": "webpack --env prod --config config/webpack.config.cs.js --profile --json > bundle-stats-cs.json",
    "analyse-bundle-cs": "webpack-bundle-analyzer bundle-stats-cs.json build-cs"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:jsx-a11y/strict"
    ],
    "plugins": [
      "react-hooks",
      "@typescript-eslint",
      "jsx-a11y"
    ],
    "rules": {
      "no-prototype-builtins": "off",
      "prefer-const": "warn",
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/no-empty-interface": "warn",
      "@typescript-eslint/explicit-module-boundary-types": "off"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-transform-classes": "^7.16.0",
    "@babel/plugin-transform-modules-commonjs": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@types/he": "^1.1.2",
    "@types/history": "^4.7.11",
    "@types/html-to-text": "^8.0.1",
    "@types/jest": "^27.0.3",
    "@types/js-cookie": "^3.0.1",
    "@types/katex": "^0.11.1",
    "@types/lodash": "^4.14.177",
    "@types/qrcode": "^1.4.1",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^17.0.11",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/redux-logger": "^3.0.9",
    "@types/redux-mock-store": "^1.0.3",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "babel-loader": "^8.2.3",
    "bootstrap": "^4.6.1",
    "bootstrap-block-grid": "^1.1.7",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.4.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.4.7",
    "mini-css-extract-plugin": "^2.4.5",
    "sass": "^1.44.0",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.2.5",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  }
}
