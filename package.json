{
  "name": "isaac-react-app",
  "version": "3.8.2-SNAPSHOT",
  "private": true,
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "engineStrict": true,
  "dependencies": {
    "@dnd-kit/core": "^6.0.5",
    "@dnd-kit/sortable": "^7.0.1",
    "@dnd-kit/utilities": "^3.2.0",
    "@reduxjs/toolkit": "^1.8.3",
    "axios": "^0.27.2",
    "billboard.js": "^1.12.11",
    "core-js": "^3.35.1",
    "dayjs": "^1.11.10",
    "he": "^1.2.0",
    "highlight.js": "^11.9.0",
    "history": "^4.10.1",
    "html-to-text": "^9.0.5",
    "identity-obj-proxy": "3.0.0",
    "immer": "^9.0.15",
    "inequality": "1.0.1",
    "inequality-grammar": "1.1.1",
    "isaac-graph-sketcher": "0.13.7",
    "js-cookie": "^3.0.1",
    "katex": "^0.16.9",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.21",
    "object-hash": "^3.0.0",
    "p5": "^1.9.0",
    "plausible-tracker": "^0.3.8",
    "qrcode": "^1.5.3",
    "query-string": "^7.0.1",
    "rand-seed": "^1.0.1",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-circular-progressbar": "^2.0.4",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-select": "^5.4.0",
    "react-window": "^1.8.9",
    "reactstrap": "^8.10.1",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "remarkable": "^2.0.1",
    "uuid": "^9.0.1"
  },
  "scripts": {
    "build-phy": "webpack --env prod --config config/webpack.config.physics.js",
    "build-ada": "webpack --env prod --config config/webpack.config.ada.js",
    "build-phy-dev": "webpack --config config/webpack.config.physics.js",
    "build-ada-dev": "webpack --config config/webpack.config.ada.js",
    "start-phy": "webpack-dev-server --hot --port 8004 --history-api-fallback --config config/webpack.config.physics.js --allowed-hosts=true",
    "start-ada": "webpack-dev-server --hot --port 8003 --history-api-fallback --config config/webpack.config.ada.js --allowed-hosts=true",
    "test": "yarn run test-phy && yarn run test-ada",
    "test-phy": "jest --config config/jest/jest.config.physics.js",
    "test-ada": "jest --config config/jest/jest.config.ada.js",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "build-stats-phy": "webpack --env prod --config config/webpack.config.physics.js --profile --json > bundle-stats-phy.json",
    "analyse-bundle-phy": "webpack-bundle-analyzer bundle-stats-phy.json build-physics",
    "build-stats-ada": "webpack --env prod --config config/webpack.config.ada.js --profile --json > bundle-stats-ada.json",
    "analyse-bundle-ada": "webpack-bundle-analyzer bundle-stats-ada.json build-ada",
    "build-ada-renderer": "webpack --env prod --config config/webpack.config.ada.renderer.js",
    "start-ada-renderer": "webpack-dev-server --hot --port 3001 --history-api-fallback --config config/webpack.config.ada.renderer.js --allowed-hosts=true",
    "build-phy-renderer": "webpack --env prod --config config/webpack.config.phy.renderer.js",
    "start-phy-renderer": "webpack-dev-server --hot --port 3002 --history-api-fallback --config config/webpack.config.phy.renderer.js --allowed-hosts=true",
    "build-renderer": "yarn run build-ada-renderer && yarn run build-phy-renderer"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:jsx-a11y/strict"
    ],
    "plugins": [
      "react-hooks",
      "@typescript-eslint",
      "jsx-a11y"
    ],
    "rules": {
      "no-prototype-builtins": "off",
      "prefer-const": "warn",
      "semi": "warn",
      "jsx-a11y/no-static-element-interactions": "warn",
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn",
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ],
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/no-empty-interface": "warn",
      "@typescript-eslint/explicit-module-boundary-types": "off"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie > 0",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/plugin-transform-classes": "^7.23.8",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.5.2",
    "@types/he": "^1.2.3",
    "@types/history": "^4.7.11",
    "@types/html-to-text": "^9.0.4",
    "@types/jest": "^29.5.12",
    "@types/js-cookie": "^3.0.1",
    "@types/katex": "^0.16.7",
    "@types/leaflet": "^1.7.9",
    "@types/lodash": "^4.14.202",
    "@types/object-hash": "^3.0.2",
    "@types/qrcode": "^1.5.5",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.5",
    "@types/react-leaflet": "^2.8.2",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-window": "^1.8.5",
    "@types/redux-logger": "^3.0.9",
    "@types/redux-mock-store": "^1.0.3",
    "@types/remarkable": "^2.0.8",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "axios-mock-adapter": "^1.22.0",
    "babel-loader": "^9.1.3",
    "bootstrap": "^4.6.1",
    "bootstrap-block-grid": "^1.1.7",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.10.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.4.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "graphql": "^16.6.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "mini-css-extract-plugin": "^2.8.0",
    "msw": "0.49.0",
    "react-app-polyfill": "^3.0.0",
    "react-leaflet": "^2.8.0",
    "react-remove-props-loader": "^1.1.8",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.4.1",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.0",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.9.5",
    "webpack": "^5.90.1",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
